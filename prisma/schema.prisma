// ========== 알림 시스템 ==========

model approval_notifications {
  id                String         @id @default(uuid()) @db.Uuid
  recipient_id      String         @db.Uuid
  sender_id         String?        @db.Uuid
  new_user_id       String         @db.Uuid
  new_user_email    String
  new_user_name     String
  new_user_region   String?
  new_user_org      String?
  notification_type String         @db.VarChar(50) // 'new_signup', 'reminder'
  is_read           Boolean        @default(false)
  read_at           DateTime?      @db.Timestamptz(6)
  created_at        DateTime       @default(now()) @db.Timestamptz(6)

  recipient         user_profiles  @relation("approval_notifications_recipient", fields: [recipient_id], references: [id], onDelete: Cascade)
  sender            user_profiles? @relation("approval_notifications_sender", fields: [sender_id], references: [id], onDelete: SetNull)

  @@index([recipient_id, is_read, created_at], map: "idx_approval_notif_recipient")
  @@index([new_user_id], map: "idx_approval_notif_new_user")
  @@index([created_at], map: "idx_approval_notif_created_at")
}

model notification_settings {
  id                    String        @id @default(uuid()) @db.Uuid
  user_id               String        @unique @db.Uuid
  sound_enabled         Boolean       @default(true)
  voice_enabled         Boolean       @default(true)
  browser_push_enabled  Boolean       @default(true)
  toast_enabled         Boolean       @default(true)
  reminder_enabled      Boolean       @default(true)
  reminder_interval     Int           @default(60) // 초 단위 (기본: 60초 = 1분)
  quiet_hours_enabled   Boolean       @default(false)
  quiet_hours_start     String?       @db.VarChar(5) // "22:00" 형식
  quiet_hours_end       String?       @db.VarChar(5) // "08:00" 형식
  created_at            DateTime      @default(now()) @db.Timestamptz(6)
  updated_at            DateTime      @default(now()) @updatedAt @db.Timestamptz(6)

  user                  user_profiles @relation(fields: [user_id], references: [id], onDelete: Cascade)

  @@index([user_id], map: "idx_notification_settings_user")
}

model push_subscriptions {
  id           String        @id @default(uuid()) @db.Uuid
  user_id      String        @db.Uuid
  endpoint     String        @db.VarChar(500)
  p256dh_key   String        @db.VarChar(200)
  auth_key     String        @db.VarChar(200)
  user_agent   String?       @db.VarChar(500)
  created_at   DateTime      @default(now()) @db.Timestamptz(6)
  updated_at   DateTime      @default(now()) @updatedAt @db.Timestamptz(6)
  last_used_at DateTime?     @db.Timestamptz(6)

  user         user_profiles @relation(fields: [user_id], references: [id], onDelete: Cascade)

  @@unique([user_id, endpoint])
  @@index([user_id], map: "idx_push_subscriptions_user")
  @@index([endpoint], map: "idx_push_subscriptions_endpoint")
}
